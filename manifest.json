{
  "name": "Youtube Frontpage Redirect",
  "description": "Redirect youtube.com to https://www.youtube.com/feed/subscriptions",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "*://youtube.com/",
    "webRequestBlocking"
  ]
}
